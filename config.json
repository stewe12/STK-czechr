{
  "name": "STK czechr",
  "version": "1.0",
  "slug": "stk_czechr",
  "description": "Fetches car inspection data and exposes it as entities.",
  "arch": ["amd64", "armv7", "armhf"],
  "startup": "services",
  "boot": "auto",
  "options": {
    "cars": [
      {
        "name": "Car 1",
        "vin": "1HGBH41JXMN109186"
      },
      {
        "name": "Car 2",
        "vin": "1HGBH41JXMN109187"
      }
    ]
  },
  "schema": {
    "cars": [
      {
        "name": "name",
        "required": true,
        "type": "str"
      },
      {
        "name": "vin",
        "required": true,
        "type": "str",
        "length": 17
      }
    ]
  },
  "ports": {
    "80/tcp": 8080
  },
  "webui": "http://[HOST]:[PORT:8080]",
  "ingress": true,
  "ingress_port": 8099,
  "ingress_entry": "/",
  "panel_icon": "mdi:car"
}
